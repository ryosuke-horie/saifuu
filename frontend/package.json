{
	"name": "saifuu",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"dev:e2e": "next dev --turbopack --port 3002",
		"build": "NODE_ENV=production next build",
		"build:cf": "NODE_ENV=production opennextjs-cloudflare build",
		"start": "next start",
		"lint": "next lint",
		"lint:biome": "biome check .",
		"lint:biome:fix": "biome check --fix .",
		"format": "biome format --write .",
		"typecheck": "tsc --noEmit",
		"test": "vitest",
		"test:unit": "vitest run",
		"test:unit:coverage": "vitest run --coverage",
		"test:watch": "vitest watch",
		"check": "npm run typecheck && npm run lint:biome && npm run test:unit",
		"check:fix": "npm run typecheck && npm run lint:biome:fix && npm run test:unit",
		"ci:check": "npm run typecheck && npm run lint:biome && npm run test:unit",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"deploy": "opennextjs-cloudflare deploy",
		"deploy:manual": "NODE_ENV=production opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"preview": "NODE_ENV=production opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:report": "playwright show-report",
		"test:visual": "ENABLE_VISUAL_TESTS=true vitest run --config vitest.config.ts --reporter=verbose",
		"test:visual:update": "ENABLE_VISUAL_TESTS=true vitest run --config vitest.config.ts --reporter=verbose --update-snapshots",
		"baseline:status": "scripts/baseline-manager.sh status",
		"baseline:create": "scripts/baseline-manager.sh create",
		"baseline:update": "scripts/baseline-manager.sh update --backup",
		"baseline:clean": "scripts/baseline-manager.sh clean",
		"baseline:validate": "scripts/baseline-manager.sh validate",
		"baseline:diff": "scripts/baseline-manager.sh diff",
		"baseline:backup": "scripts/baseline-manager.sh backup",
		"baseline:help": "scripts/baseline-manager.sh help",
		"ci:visual": "scripts/ci-baseline-manager.sh test",
		"ci:baseline:create": "scripts/ci-baseline-manager.sh create",
		"ci:baseline:update": "scripts/ci-baseline-manager.sh update",
		"visual:integrate": "scripts/integrate-visual-tests.sh all",
		"visual:integrate:test": "scripts/integrate-visual-tests.sh run-test",
		"visual:integrate:artifacts": "scripts/integrate-visual-tests.sh artifacts"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "^1.3.1",
		"next": "15.3.4",
		"react": "^19.1.0",
		"react-dom": "^19.1.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.6",
		"@faker-js/faker": "^9.9.0",
		"@playwright/test": "^1.53.1",
		"@storybook/addon-a11y": "^8.6.14",
		"@storybook/addon-actions": "^8.6.14",
		"@storybook/addon-essentials": "^8.6.14",
		"@storybook/addon-viewport": "^8.6.14",
		"@storybook/nextjs": "^8.6.14",
		"@storybook/react": "^8.6.14",
		"@storybook/test": "^8.6.14",
		"@tailwindcss/postcss": "^4",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/node": "^24.0.7",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@vitejs/plugin-react": "^4.6.0",
		"@vitest/browser": "^3.2.4",
		"@vitest/coverage-v8": "^3.2.4",
		"jsdom": "^26.1.0",
		"msw": "^2.10.2",
		"msw-storybook-addon": "^2.0.5",
		"storybook": "^8.6.14",
		"storybook-addon-vis": "^1.1.2",
		"tailwindcss": "^4",
		"typescript": "^5",
		"vitest": "^3.2.4",
		"wrangler": "^4.22.0"
	},
	"msw": {
		"workerDirectory": [
			"public"
		]
	}
}
