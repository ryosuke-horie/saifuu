{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"lib": ["ESNext"],
		"types": ["vite/client", "@cloudflare/workers-types", "@types/better-sqlite3", "@types/node"],
		"jsx": "react-jsx",
